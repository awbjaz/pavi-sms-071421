<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record model="crm.stage" id="stage_lead2">
        <field name="name">Serviceability/Credit Checking</field>
        <field name="sequence">2</field>
    </record>
    <record model="crm.stage" id="stage_lead3">
        <field name="name">Awaiting Feedback from Client</field>
        <field name="sequence">3</field>
    </record>
    <record model="crm.stage" id="stage_lead4">
        <field name="name">Closed Won</field>
        <field name="fold" eval="False"/>
        <field name="is_won">True</field>
        <field name="sequence">70</field>
    </record>
    <record model="crm.stage" id="stage_lead5">
        <field name="name">Closed Lost</field>
        <field name="fold" eval="True"/>
        <field name="sequence">71</field>
    </record>
    <record model="crm.stage" id="stage_completed">
        <field name="name">Completed</field>
        <field name="fold" eval="False"/>
        <field name="is_won">True</field>
        <field name="is_auto_quotation" eval="True"/>
        <field name="sequence">80</field>
    </record>
</odoo>